<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.todayhospital.mappers.MemberMapper">

    <select id="getMemberById" parameterType="String" resultType="com.todayhospital.dto.MemberDTO">
        SELECT MEM_ID AS memId, MEM_NAME AS memName
        FROM MEMBER
        WHERE MEM_ID = #{memId}
    </select>

</mapper>
